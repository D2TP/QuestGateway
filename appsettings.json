{
  "app": {
    "name": "Quest Gateway"
  },
  "Urls": "http://0.0.0.0:80",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {      
      "minimumroute": {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "onboarding",
        "Match": {
          "Path": "/onboarding/{*any}"
        },
        "Transforms": [
          {
            "PathPattern": "{*any}"
          }
        ]
      },
      "route3": {
        "ClusterId": "authentication",
        "Match": {
          "Path": "/authentication/{*any}"
        },
        "Transforms": [
          {
            "PathPattern": "{*any}"
          }
        ]
      },
      "route4": {
        "ClusterId": "common-utilities",
        "Match": {
          "Path": "/common/{*any}"
        },
        "Transforms": [
          {
            "PathPattern": "{*any}"
          }
        ]
      }
    },
    "Clusters": {      
      "minimumcluster": {
        "Destinations": {
          "example.com": {
            "Address": "http://10.100.33.114:5007/"
          }
        }
      },
      "onboarding": {
        "Destinations": {
          "onboarding/destination1": {
            "Address": "http://10.100.33.114:5008/"
          }
        }
      },
      "authentication": {
        "Destinations": {
          "authentication/destination1": {
            "Address": "http://10.100.33.114:5007/"
          }
        }
      },
      "common-utilities": {
        "Destinations": {
          "common/destination1": {
            "Address": "http://10.100.33.114:5006/"
          }
        }
      }
    }
  }
}